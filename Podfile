# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Familiar' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Familiar
  pod 'Alamofire'	# 통신을 위한 라이브러리
  pod 'KakaoSDKCommon'	# kakao 필수 요소 모듈
  pod 'KakaoSDKAuth'	# kakao 사용자 인증
  pod 'KakaoSDKUser'	# kakao 로그인, 사용자 관리

end
